{
    "extends": [
      "react-app", "airbnb", "prettier"
    ],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": ["error", {
            "endOfLine": "auto"
          }],
        "react/jsx-uses-react": "off",
        "react/react-in-jsx-scope": "off",
        "react/prop-types": "off"
        
    }
}